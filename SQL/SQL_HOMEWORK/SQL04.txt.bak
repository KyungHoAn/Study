////////1번
SELECT
J.JOB_TITLE, SUM(SALARY)
FROM EMPLOYEES E, JOBS J
WHERE E.JOB_ID = J.JOB_ID
GROUP BY J.JOB_TITLE
HAVING SUM(SALARY)>30000
ORDER BY SUM(SALARY) DESC;


