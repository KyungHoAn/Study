SELECT
E.EMPLOYEE_ID, E.FIRST_NAME, E.LAST_NAME, E.EMAIL, E.PHONE_NUMBER, E.HIRE_DATE, E.JOB_ID, E.SALARY, E.COMMISSION_PCT, E.MANAGER_ID, E.DEPARTMENT_ID
FROM EMPLOYEES E;


CREATE TABLE EMPLOYEES_MARKETING(
	EMPLOYEE_ID		INT
	FIRST_NAME	 VARCHARACTER
	LAST_NAME	VARCHARACTER
	EMAIL		EMAIL
	PHONE_NUMBER	INT
	HIRE_DATE	DATE
	JOB_ID		INT
	SALARY		INT
	COMMISSION_PCT		INT
	MANAGER_ID			INT
	DEPARTMENT_ID		INT
);