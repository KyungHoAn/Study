SELECT
EMPLOYEE_ID, LAST_NAME, MANAGER_ID
FROM EMPLOYEES
WHERE MANAGER_ID = (SELECT
EMPLOYEE_ID
FROM EMPLOYEES); 



SELECT
LAST_NAME, SALARY
FROM EMPLOYEES

SELECT
SALARY
EMPLOYEE_ID, MANAGER_ID
FROM EMPLOYEES


SELECT
LAST_NAME,SALARY
FROM EMPLOYEES, EMPLOYEES E2
WHERE E1.EMPLOYEE_ID = E2.MANAGER_ID AND E1.SALARY>E2.SALARY;