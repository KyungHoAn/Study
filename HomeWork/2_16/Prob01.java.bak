import java.util.Scanner;

public class Prob01 {
	// Main Method
	public static void main(String[] args) {
		///
		// int num = Integer.parseInt(args[0]);

		Scanner stdIn = new Scanner(System.in);

		System.out.println("숫자 입력: ");
		int num = stdIn.nextInt();

		char star = '*';
		for(int i=0; i<num; i++) {
			for(int j=0; j<num; j++) {
				System.out.print(star);
				
				if(j == (num-1)) {
					System.out.println();
					num-=2;
				}
			}
		}
		
